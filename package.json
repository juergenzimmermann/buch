{
  "name": "buch",
  "version": "2026.4.1",
  "private": true,
  "description": "Beispiel",
  "keywords": [
    "Node",
    "pnpm",
    "Nest",
    "REST",
    "GraphQL",
    "Apollo",
    "TypeScript",
    "ESLint",
    "Prettier",
    "PostgreSQL",
    "Prisma",
    "JWT",
    "Keycloak",
    "class-validator",
    "CSP",
    "Clickjacking",
    "CORS",
    "pino",
    "nodemailer",
    "vitest",
    "k6",
    "typedoc"
  ],
  "license": "GPL-3.0",
  "author": "Juergen Zimmermann <Juergen.Zimmermann@h-ka.de> (https://www.h-ka.de)",
  "type": "module",
  "exports": {
    ".": "./dist/src/main.js"
  },
  "scripts": {
    "prebuild": "pnpm run clean",
    "build": "nest build",
    "start": "node --env-file=.env dist/src/main.js",
    "predev": "pnpm run copy-resources",
    "dev": "nest start --env-file=.env --watch",
    "predev:ts": "pnpm run copy-resources",
    "dev:ts": "nest start --env-file=.env src/main.ts",
    "test": "SET NODE_TLS_REJECT_UNAUTHORIZED=0&& vitest --ui",
    "test:unit": "vitest -c ./vitest.config.unit.ts --ui",
    "test:k6": "k6 run test\\lasttest\\script.ts",
    "asciidoctor": "node scripts/asciidoctor.mts",
    "clean": "pnpm run clean:dist && pnpm run clean:log",
    "clean:dist": "node --eval=\"import fs from 'node:fs/promises'; await fs.rm('dist', { recursive: true, force: true });\"",
    "clean:log": "node --eval=\"import fs from 'node:fs/promises'; import { join } from 'node:path'; await fs.rm(join('log', 'server.log'), { force: true });\"",
    "copy-resources": "node scripts/copy-resources.mts",
    "eslint": "eslint --concurrency auto --flag v10_config_lookup_from_file src test/integration package.json",
    "eslint:inspect-config": "npx @eslint/config-inspector",
    "eslint:debug": "pnpm run eslint -- --debug",
    "leasot": "leasot --exit-nicely src/**/*.ts",
    "madge": "madge --circular src/main.ts",
    "madge:svg": "madge --image dependencies.svg src/main.ts",
    "prettier": "prettier --cache --write \"src/**/*.ts\" \"test/integration/**/*.mts\" \"test/lasttest/*.ts\" \"package.json\"",
    "prettier:check": "prettier --cache --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "revealjs": "asciidoctor-revealjs extras/doc/folien/folien.adoc",
    "sonar": "node scripts/sonar-scanner.mts",
    "tsc": "tsc --pretty",
    "tsgo": "tsgo --project tsconfig.json"
  },
  "dependencies": {
    "@apollo/server": "5.3.0",
    "@as-integrations/express5": "1.1.2",
    "@hono/node-server": "1.19.9",
    "@js-temporal/polyfill": "0.5.1",
    "@nestjs/apollo": "13.2.3",
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.1.12",
